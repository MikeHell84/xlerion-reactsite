    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^4.1.18"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.0.0",
    "sass": "^1.73.0",
    "vite": "^5.0.0",
    "postcss-cli": "^10.1.0"
  }
}